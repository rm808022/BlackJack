<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners
        android:radius="2dp" />

    <solid android:color="#191970" />
    <stroke android:width="2dp"
        android:color="#ffffff"/>

    <padding
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp"/>

    <size
        android:width="80dp"
        android:height="30dp"
        />

    <gradient
        android:startColor="#012c64"
        android:centerColor="#1874f7"
        android:endColor="#012c64"
        android:angle="90"/>
</shape>